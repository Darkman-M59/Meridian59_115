PrincessGuard is BarloqueTown

constants:

   include blakston.khd

resources:

   princess_guard_name_rsc = "Die Palastwache"
   princess_guard_icon_rsc = pringard.bgf
   princess_guard_desc_rsc = "Ihr Bestes gebend, um so würdevoll auszusehen wie die königliche Hoheit, die er zu beschützen hat trägt diese Wache seine Uniform mit Stolz. Das Gefühl von Sicherheit erfüllt den Bereich um die Hofwache."






classvars:
   vrName = princess_guard_name_rsc
   vrIcon = princess_guard_icon_rsc
