   viAttributes = MOB_NOFIGHT | MOB_NOMOVE | MOB_LISTEN | MOB_RANDOM | MOB_NOQUEST
   viOccupation = MOB_ROLE_GUARD

%   viAttributes = MOB_NOFIGHT  | MOB_RECEIVE | MOB_NOMOVE

properties:

   piAnimation = ANIM_NONE
   
messages:


end
