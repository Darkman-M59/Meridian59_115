      oBook = Create(&BookPedestal,#Name=thanks_title,#Text=thanks_desc);
      Send(self,@NewHold,#what=oBook,#new_row=7,#new_col=7,
           #fine_row=16,#fine_col=32,#new_angle=ANGLE_SOUTH);

      propagate;
   }

